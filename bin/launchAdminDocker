#!/bin/bash

function dkHelp () {
  echo "Dockerizer command: launchAdminDocker"
  echo
  echo "Docker UI. Gives you an overview of all dockerizer projects and manage them."
}

case "$1" in
  --dkhelp)
    dkHelp
    exit
    ;;
esac

echo -e "Launch http://portainer.localhost in browser."
x-www-browser "http://portainer.localhost"
